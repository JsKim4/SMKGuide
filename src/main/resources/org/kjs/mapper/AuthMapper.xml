<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.kjs.mapper.AuthMapper">
	<insert id="insert">
		insert into tbl_auth(member_id,auth) values(#{memberId},#{auth})
	</insert>
	
	
	
	<delete id="delete">
		delete from tbl_auth where member_id = #{memberId}
	</delete>
	
	<select id="getList" resultType="org.kjs.domain.AuthVO">
		select member_id memberId,auth from tbl_auth where member_id = #{memberId}
	</select>
	
</mapper>